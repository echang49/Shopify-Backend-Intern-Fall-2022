  "devDependencies": {
    "@shopify/eslint-plugin": "^41.2.1",
    "@shopify/prettier-config": "^1.1.2",
    "@types/express": "^4.17.13",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^17.0.35",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
